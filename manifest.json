{
   "manifest_version": 3,
   "name": "Favorites - New Tab Page",
   "version": "1.18.0",
   "description": "__MSG_extension_description__",
   "default_locale": "en",
   "minimum_chrome_version": "88.0.0",
   "icons": {
      "16": "icon/icon-16.png",
      "24": "icon/icon-24.png",
      "32": "icon/icon-32.png",
      "48": "icon/icon-48.png",
      "128": "icon/icon-128.png"
   },
   "action": {
      "default_icon": {
         "16": "icon/icon-16.png",
         "24": "icon/icon-24.png",
         "32": "icon/icon-32.png",
         "48": "icon/icon-48.png",
         "64": "icon/icon-64.png",
         "128": "icon/icon-128.png"
      },
      "default_title": "Favorites"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "chrome_url_overrides": {
      "newtab": "page.html?theme&ui=NewtabUI&title=new_tab"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
   },
   "permissions": [
      "activeTab",
      "contextMenus",
      "bookmarks",
      "alarms",
      "storage",
      "unlimitedStorage",
      "scripting",
      "notifications",
      "tabs"
   ],
   "host_permissions": [
      "http://*/*",
      "https://*/*"
   ],
   "optional_permissions": [
      "clipboardRead",
      "clipboardWrite"
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "page.html",
            "ui/**",
            "icons/**",
            "images/**",
            "wallpapers/**"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ],
   "incognito": "spanning",
   "short_name": "Favorites",
   "update_url": "https://clients2.google.com/service/update2/crx"
}
